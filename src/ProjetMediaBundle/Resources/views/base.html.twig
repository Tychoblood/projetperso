<!doctype html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7" lang=""> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8" lang=""> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9" lang=""> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" lang=""> <!--<![endif]-->
<html>
	<head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
		<title>{% block title %}Page de lancement{% endblock %}</title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="apple-touch-icon" href="apple-touch-icon.png">
		{% stylesheets
			'css/bootstrap.min.css'
			'css/bootstrap-theme.min.css'
			'css/font-awesome.css'
			'css/bootstrap-social.css'
			'css/main.css'
			filter='cssrewrite' output='css/compiled/app.css' %}
			<link rel="stylesheet" href="{{ asset_url }}" />
		{% endstylesheets %}
    </head>
	<body>
        <!--[if lt IE 8]>
            <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
        <![endif]-->
		
    <nav class="navbar navbar-inverse navbar-static-top" role="navigation">
      <div class="container">
        <div class="navbar-header">
		  <a class="navbar-brand" href="{{  url('urldebase') }}">Projet Media</a>
			{% if logoutUrl is defined %}
				 <a href="{{ logoutUrl }}" target="_blank" class="btn btn-block btn-social btn-facebook">
				  <span class="fa fa-facebook"></span>
				  Se d√©connecter
				</a>
			{% endif %}

        </div>
      </div>
    </nav>

    <!-- Main jumbotron for a primary marketing message or call to action -->
    <div class="jumbotron">
      <div class="container">
		{% block greetings %}
			{% if logoutUrl is not defined %}
				<h2>Bonjour,  </h2>
				<p>veuillez vous connecter ci-dessous :</p>
			{% endif %}
		{% endblock %}
      </div>
    </div>

    <div class="container">
      <!-- Example row of columns -->
	  
      <div class="row">
		{% block body %}
		{% endblock %}
      </div>

      <hr>

      <footer>
        <p>&copy; Run4Web 2016</p>
      </footer>
    </div> <!-- /container -->       
	{% javascripts
		'js/vendor/modernizr-2.8.3-respond-1.4.2.min.js'
		'https://js.pusher.com/3.2/pusher.min.js'
		'//ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js'
		'js/vendor/bootstrap.min.js'
		'js/main.js'
		output='js/compiled/app.js' %}
		<script src="{{ asset_url }}"></script>
	{% endjavascripts %}
        <script>window.jQuery || document.write('<script src="js/vendor/jquery-1.11.2.min.js"><\/script>')</script>
        <!-- Google Analytics: change UA-XXXXX-X to be your site's ID. -->
        <script>
            (function(b,o,i,l,e,r){b.GoogleAnalyticsObject=l;b[l]||(b[l]=
            function(){(b[l].q=b[l].q||[]).push(arguments)});b[l].l=+new Date;
            e=o.createElement(i);r=o.getElementsByTagName(i)[0];
            e.src='//www.google-analytics.com/analytics.js';
            r.parentNode.insertBefore(e,r)}(window,document,'script','ga'));
            ga('create','UA-46144540-1','auto');ga('send','pageview');
        </script>
		{% block scriptFadeIn %}
		
		{% endblock%}
			
    </body>
</html>

